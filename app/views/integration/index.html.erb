            <hr class="hr_social">
            <p style="font-size: 15px">Connected</p>
        </div>
          <div class="flex-shrink-0 float-start social_button_align">
            <%= link_to "Disconnect", disconnect_facebook_path, id: "google", class: "btn rounded btn-lg social_connect_button" %>
          </div>
        </div>
            <%else %>
            <div class="flex-grow-1 ms-5" >
              <h3 style="font-size: 20px">Facebook</h3>
