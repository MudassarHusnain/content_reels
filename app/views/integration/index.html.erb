              <p style="font-size: 15px">Not Connected</p>
            </div>
            <div class="flex-shrink-0 float-start social_button_align">
              <%= button_to "Connect", user_facebook_omniauth_authorize_path , method: :post, data: { turbo: "false" }, id: "google", class: "btn rounded btn-lg social_connect_button"%>
            </div>
            </div>
